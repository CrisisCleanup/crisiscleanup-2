      <th>Last Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Title</th>
      <th>Organization</th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td>
          	<%= contact.first_name %>
        </td>
        <td> 
        	<%= contact.last_name %> 
        </td>
